public class Shape extends  ShapeComponent {

	private int count;
	private int size;
	
	public Shape(int count,int size) 
	{
		this.count= count;
		this.size = size;
	}

 
	

	public int getCount() { return count;}
	public int getSize() {return size;}




	
}
